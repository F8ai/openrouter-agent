{
  "name": "openrouter-agent",
  "version": "1.0.0",
  "description": "OpenRouter API key management agent for Formul8 ecosystem",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "build": "echo 'No build step required for Node.js'",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "openrouter",
    "api-keys",
    "key-management",
    "formul8",
    "multiagent",
    "supabase"
  ],
  "author": "F8ai",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/F8ai/openrouter-agent.git"
  },
  "bugs": {
    "url": "https://github.com/F8ai/openrouter-agent/issues"
  },
  "homepage": "https://github.com/F8ai/openrouter-agent#readme"
}
